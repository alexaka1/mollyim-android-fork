<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Disabled -->
    <item android:alpha="@dimen/material_emphasis_disabled" android:color="?attr/colorOutline" android:state_enabled="false"/>

    <!-- Checked -->
    <item android:color="?attr/colorPrimary" android:state_checked="true"/>

    <!-- Unchecked -->
    <item android:color="?attr/colorOutlineVariant" android:state_checked="false"/>
</selector>